{
  "testMate.cpp.test.advancedExecutables": [
    {
      "pattern": "{build,Build,BUILD}/cpp-practice{,.exe}",
      "cwd": "${workspaceFolder}",
      "env": {
        "TEST": "true"
      },
      "runTask": {
        "before": ["npm: beforeTest"]
        // "after": ["npm: afterTest"]
      },
      "executionWrapper": {
        "path": "${workspaceFolder}/scripts/unix-wrapper.sh",
        "args": [
          "-c",
          "\"${cmd}\" ${argsStr} 2>&1",
          "-l",
          "${workspaceFolder}/build"
        ]
      },
      "win32": {
        "executionWrapper": {
          "path": "${workspaceFolder}/scripts/executionWrapper.exe",
          "args": [
            "-c",
            "\"${cmd}\" ${argsStr} 2>&1",
            "-l",
            "${workspaceFolder}/build"
          ]
        }
      }
    }
  ],

  "clangd.checkUpdates": true,
  "cmake.outputLogEncoding": "utf8",
  "cmake.statusbar.advanced": {
    "ctest": {
      "visibility": "hidden"
    }
  },
  "coverage-gutters.coverageBaseDir": "**",
  "coverage-gutters.coverageFileNames": ["lcov", "lcov.info"],
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "googletest.failed",
        "settings": {
          "foreground": "#f00"
        }
      },
      {
        "scope": "googletest.passed",
        "settings": {
          "foreground": "#0f0"
        }
      },
      {
        "scope": "googletest.run",
        "settings": {
          "foreground": "#0f0"
        }
      }
    ]
  },
